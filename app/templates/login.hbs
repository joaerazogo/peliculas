<div class="row">
    <form class="col s12">
      <div class="row">
        <div class="input-field col s6">
          {{input type="text" id="username" value=userName name =userName}}
          <label for="first_name">User</label>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s6">
          {{input type="password" id="pass" value=password name=password}}
          <label for="password">Password</label>
        </div>
      </div>
      <a class="waves-effect waves-light btn" {{action 'login'}}><i class="material-icons left">send</i>Send</a>
    </form>
</div>
{{outlet}}
